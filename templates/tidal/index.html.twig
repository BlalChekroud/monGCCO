{% extends 'base.html.twig' %}

{% block title %}Tidal index{% endblock %}

{% block body %}
    <h1>Tidal index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tidal in tidals %}
            <tr>
                <td>{{ tidal.id }}</td>
                <td>{{ tidal.label }}</td>
                <td>{{ tidal.createdAt ? tidal.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ tidal.updatedAt ? tidal.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_tidal_show', {'id': tidal.id}) }}">show</a>
                    <a href="{{ path('app_tidal_edit', {'id': tidal.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_tidal_new') }}">Create new</a>
{% endblock %}
