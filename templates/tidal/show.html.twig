{% extends 'base.html.twig' %}

{% block title %}Tidal{% endblock %}

{% block body %}
    <h1>Tidal</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tidal.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ tidal.label }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ tidal.createdAt ? tidal.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ tidal.updatedAt ? tidal.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_tidal_index') }}">back to list</a>

    <a href="{{ path('app_tidal_edit', {'id': tidal.id}) }}">edit</a>

    {{ include('tidal/_delete_form.html.twig') }}
{% endblock %}
