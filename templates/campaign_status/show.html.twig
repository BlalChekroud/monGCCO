{% block title %}CampaignStatus{% endblock %}

{% block body %}
    <h1>CampaignStatus</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ campaign_status.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ campaign_status.label }}</td>
