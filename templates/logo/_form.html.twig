<turbo-frame id="logo_{{ logo.id }}">
  {{ form_start(form, {action: path('app_logo_edit', {'id': logo.id}) }) }}
      {{ form_widget(form) }}
      <div class="row mb-3">
        <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
          <button type="reset" class="btn btn-outline-dark bi bi-x-square"> Annuler</button>
        </div>
        <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
          <button class="btn btn-outline-primary bi bi-save"> {{ button_label|default('Enregistrer') }}</button>
        </div>
      </div>
  {{ form_end(form) }}
</turbo-frame>