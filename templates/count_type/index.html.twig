{% extends 'base.html.twig' %}

{% block title %}CountType index{% endblock %}

{% block body %}
    <h1>CountType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for count_type in count_types %}
            <tr>
                <td>{{ count_type.id }}</td>
                <td>{{ count_type.label }}</td>
                <td>{{ count_type.createdAt ? count_type.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ count_type.updatedAt ? count_type.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_count_type_show', {'id': count_type.id}) }}">show</a>
                    <a href="{{ path('app_count_type_edit', {'id': count_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_count_type_new') }}">Create new</a>
{% endblock %}
