{% extends 'base.html.twig' %}

{% block title %}Weather{% endblock %}

{% block body %}
    <h1>Weather</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ weather.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ weather.label }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ weather.createdAt ? weather.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ weather.updatedAt ? weather.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_weather_index') }}">back to list</a>

    <a href="{{ path('app_weather_edit', {'id': weather.id}) }}">edit</a>

    {{ include('weather/_delete_form.html.twig') }}
{% endblock %}
